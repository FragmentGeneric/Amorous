{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": ".",
  "projects": {
    "app": {
      "projectType": "application",
      "schematics": {},
      "root": "app",
      "sourceRoot": "app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../Scaledriven/wwwroot/app",
            "index": "app/src/index.html",
            "main": "app/src/main.ts",
            "polyfills": "app/src/polyfills.ts",
            "tsConfig": "app/tsconfig.app.json",
            "aot": false,
            "assets": [
              "app/src/favicon.ico",
              "app/src/assets"
            ],
            "styles": [
              "node_modules/semantic-ui-css/semantic.css",
              "app/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "app/src/environments/environment.ts",
                  "with": "app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "app/src/test.ts",
            "polyfills": "app/src/polyfills.ts",
            "tsConfig": "app/tsconfig.spec.json",
            "karmaConfig": "app/karma.conf.js",
            "assets": [
              "app/src/favicon.ico",
              "app/src/assets"
            ],
            "styles": [
              "app/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "app/tsconfig.app.json",
              "app/tsconfig.spec.json",
              "app/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "app/e2e/protractor.conf.js",
            "devServerTarget": "app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "app"
}
